   ydat6 = "['Variance Enthalpy',"
   ydat7 = "['psi Kinetic Energy',"
   ydat8 = "['ion Kinetic Energy',"
   ydat9 = "['Temperature',"
   for ee in edat.strip().split("\n"):
      ss = ee.split()
      if (len(ss)==20):
         xdat += ss[0] + ", "
         ydat1 += ss[1] + ", "
         ydat2 += ss[2] + ", "
         ydat3 += ss[12] + ", "
         ydat4 += ss[14] + ", "
         ydat5 += ss[13] + ", "
