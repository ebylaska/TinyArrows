def tamd2_plotdatajs(edat,label):

   xdat = "['t',"
   ydat1 = "['theta',"
   ydat2 = "['ztamd',"
   xx = 0.0
   for ee in edat.strip().split("\n"):
      ss = ee.split()
      if ("#" not in ss[0]):
         xdat += "%f, " % xx
         xx += 1.0
         ydat1 += ss[0] + ", "
         ydat2 += ss[1] + ", "
