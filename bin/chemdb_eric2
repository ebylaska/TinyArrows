def dipoles_plotdatajs(edat):

   xdat = "['t',"
   ydat1 = "['px',"
   ydat2 = "['py',"
   ydat3 = "['pz',"
   ydat4 = "['|p|',"
   for ee in edat.strip().split("\n"):
      ss = ee.split()
      pall = math.sqrt(evalnum(ss[1])**2 + evalnum(ss[2])**2 + evalnum(ss[3])**2)
      xdat += ss[0] + ", "
      ydat1 += ss[1] + ", "
      ydat2 += ss[2] + ", "
      ydat3 += ss[3] + ", "
      ydat4 += "%f, " % pall
   xdat  = xdat.rstrip(',') + "]"
   ydat1 = ydat1.rstrip(',') + "]"
   ydat2 = ydat2.rstrip(',') + "]"
   ydat3 = ydat3.rstrip(',') + "]"
