def plotdatajs(edat):

   eavelast = 0.0 
   evarlast = 0.0 
   havelast = 0.0 
   hvarlast = 0.0 
   templast = 0.0 
   xdat = "['x',"
   ydat1 = "['Energy',"
   ydat2 = "['Potential',"
   ydat3 = "['Average Potential',"
   ydat4 = "['Average Enthalpy',"
