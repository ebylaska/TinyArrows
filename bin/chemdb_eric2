         link3 =  ARROWS_API_HOME + 'eric_view2/' + 'download='     + ':' + projectdir.rstrip('/') + '/' +  filename.rstrip('/').rstrip('*')  
         link4 =  ARROWS_API_HOME + 'eric_view2/' + 'raw_last='     + ':' + projectdir.rstrip('/') + '/' + filename.rstrip('/').rstrip('*')  
         link5 =  ARROWS_API_HOME + 'eric_view2/' + 'xyz_raw_last=' + ':' + projectdir.rstrip('/') + '/' + filename.rstrip('/').rstrip('*')  
         ref = "<a href=\"" + link + "\">%s</a> " % filename
         ref2= "<a href=\"" + link2 + "\">%s</a>" % "(raw)"
         ref3= "<a href=\"" + link3 + "\">%s</a>" % "(download)"
         ref4= "<a href=\"" + link4 + "\">%s</a>" % "(raw_last)"
         ref5= "<a href=\"" + link5 + "\">%s</a>" % "(xyz_raw_last)"
         print(ref+ref2+ref4+ref5+ref3)
      else:
         link = ARROWS_API_HOME + 'eric_view2/' + 'view=' + ':' + projectdir.rstrip('/') + '/' + filename.rstrip('/').rstrip('*')  
         link2 = ARROWS_API_HOME + 'eric_view2/' + 'raw=' + ':' + projectdir.rstrip('/') + '/' + filename.rstrip('/').rstrip('*')  
