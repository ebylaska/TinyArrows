#### smiles2cml function ####
def smiles2cml(smiles,cmlfile):
   eoln = "\n"
   smilefile = "tmpsmiles.smiles"
   ofile = open(smilefile,'w')
   ofile.write(smiles); ofile.write(eoln)
   ofile.close()
   cmd6 = babel + " --gen3d -ismi " + smilefile + " -ocml " + cmlfile + " >& junk.err"
   os.system(cmd6)

