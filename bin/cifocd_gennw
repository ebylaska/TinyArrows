#### cml2smiles function ####
def cml2smiles(cmlfile):
   smiles = "nosmiles"
   smilefile = "tmpsmiles.smiles"
   cmd6 = babel + " -icml " + cmlfile + " -osmi " + smilefile + " >& junk.err"
   os.system(cmd6)
   sdat = []
   ofile = open(smilefile,'r')
   for line in ofile:
      sdat.append(line)
   ofile.close()
   smiles = sdat[0].split()[0]
   return smiles

