i = ocdlist.index("_cell_angle_beta");  beta  = ocdlist[i+1].split('(')[0]
i = ocdlist.index("_cell_angle_gamma"); gamma = ocdlist[i+1].split('(')[0]

ocdlist2 = ocdstring.replace('\n',' xxeolnxx ').split()
i = ocdlist2.index("_symmetry_space_group_name_H-M"); 
i += 1
groupname = ''
while (ocdlist2[i].find("xxeolnxx")==-1):
   groupname += ocdlist2[i]
   i += 1

try:
   if (groupname=='P1'):
      groupnumber = '1'
   else:
      i = ocdlist.index("_space_group_IT_number"); groupnumber = ocdlist[i+1]
except:
   groupnumber = 1


