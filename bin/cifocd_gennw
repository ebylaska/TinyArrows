try:
   symops=[]
   i = ocdlist.index("_symmetry_equiv_pos_as_xyz") + 1;
   while (ocdlist[i] != "loop_"):
      symops.append(ocdlist[i])
      i += 1
   j=0
   groupfound = False
   while ((not groupfound) and (j<230)):
      groupfound = compare_ops(symall[j][4:],symops)
      j += 1
   if (groupfound):
      setting = symall[j-1][1]
      gname = symall[j-1][2]
   else:
      gname = groupname.replace(" ","").replace("\'","")
except:
   groupfound = False
   gname = groupname.replace(" ","").replace("\'","")
   symops = []



#print("symops=",symops)
loops = []
for i in range(len(ocdlist)):
   if (ocdlist[i]=='loop_'): 
      loops.append(i)

#print(ocdlist)
try:
   isym = ocdlist.index("_atom_site_type_symbol")
except:
   isym = ocdlist.index("_atom_site_label")
ix = ocdlist.index('_atom_site_fract_x') 
iy = ocdlist.index('_atom_site_fract_y') 
iz = ocdlist.index('_atom_site_fract_z') 

if (ix>loops[len(loops)-1]):
   ilx = len(loops)-1
else:
   done = False
   ilx = 0 
   while (not done):
     if ((loops[ilx]<ix) and (loops[ilx+1]>ix)): 
        done = True
     else:
        ilx += 1



i         = loops[ilx]+1
isym_indx = isym-i
ix_indx   = ix - i
iy_indx   = iy - i
iz_indx   = iz - i
il = 0
while (ocdlist[i][0]=='_'):
   i  += 1
   il += 1

iend = len(ocdlist)
if (ilx<(len(loops)-1)):
   iend = loops[ilx+1]

fracgeom=[]
done = False
while (not done):
   for j in range(il):
       if (i>=iend): break
       ss = ocdlist[i]
       i += 1
       if (j==isym_indx): symbol=ss
       if (j==ix_indx):   x = ss.split('(')[0]
       if (j==iy_indx):   y = ss.split('(')[0]
       if (j==iz_indx):   z = ss.split('(')[0]
   fracgeom.append(symbol+' '+x+' '+y+' '+z)
   done = (i>=iend)

   

