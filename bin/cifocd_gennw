def xyz_molecular_formula(xyzfilename):
   #
   fdict = {}
   xyzfile = open(xyzfilename,'r')
   n = eval(xyzfile.readline())
   xyzfile.readline()
   for i in range(n):
      line = xyzfile.readline()
      if (line[1]==' '):
         key = line[0]
      else:
         key = line[0:2]
      if (fdict.has_key(key)):
         fdict[key] += 1
      else:
         fdict[key] = 1
   xyzfile.close()

   formula = ''
   ll = fdict.items()
   for i in range(len(ll)):
      formula += ll[i][0] + "%d" % ll[i][1]
   return formula



def gennum(v):
   if (v==''):
      x = 1.0
   else:
      vv = v.split('/')
      x = eval(vv[0])
      if (len(vv)>1):
         x = float(x)/float(eval(vv[1]))
   return x


def opmatrix(xyzstr):
   op = 12*[0.0]
   for ijk in range(3):
      xs = xyzstr.split(',')[ijk]
      pp = xs.split('+')
      pm = []
      for pi in pp:
         pm.append(pi.split('-'))

      for pi in pm:
         sgn = +1
         for mi in pi:
            if (mi.find('x')!=-1):
               v = mi.split('x')[0]
               op[ijk] += sgn*gennum(v)
            elif (mi.find('y')!=-1):
               v = mi.split('y')[0]
               op[ijk+3] += sgn*gennum(v)
            elif (mi.find('z')!=-1):
               v = mi.split('z')[0]
               op[ijk+6] += sgn*gennum(v)
            else:
               v = mi
               if (v!=''): 
                  op[ijk+9] += sgn*gennum(v)
            sgn = -1
   return op


def compare_ops(ops1,ops2):
   same = False
   if (len(ops1)==len(ops2)):
      for op1 in ops1:
         m1 = opmatrix(op1)
         same = False
         for op2 in ops2:
            m2 = opmatrix(op2)
            same = same or (m1==m2)
         if (not same):
            break
   return same





