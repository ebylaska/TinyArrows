else:
   cifocd  = sys.argv[parg]
   parg = parg + 1
   argcount = argcount - 1 

if (os.path.isfile(cifocd)):
   ofile = open(cifocd,'r')
   ocdstring = ofile.read()
   ofile.close()
else:
   ocdstring = ocd_get(cifocd)


tmp = ocdstring.strip().split('\n')
while  "_" in tmp[len(tmp)-1]:
   jj = tmp.pop()
ocdstring =  "\n".join(tmp) + "\n"

ocdlist = ocdstring.split()

i = ocdlist.index("_cell_length_a"); a = ocdlist[i+1].split('(')[0]
