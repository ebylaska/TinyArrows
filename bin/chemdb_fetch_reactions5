    var %s = c3.generate({
       bindto: '#%s',
       size: { height: 480, width: 800 },
       data: {
         //type: 'spline',
         x: '%s',
         columns: [
           %s  ''' % (title,chart,chart,chart,xlabel,xdat)
   for i in range(ny): msg4 += ", %s" % (ydat[i])
