def cleanfoo(foo):
   foo = foo.replace('(','').replace(')','')
   while ("xyzdata{" in foo) and ("}" in foo):
      msg0 = foo.split("xyzdata{")[1].split("}")[0]
      foo  = foo.replace('xyzdata{' + msg0 + '}','')
   return foo

