   if (xlabel0==''): xlabel0=xlabel
   if (ylabel0==''): ylabel0=ylabel
   for ee in elist:
      ss = ee.split()
      if ("#" not in ss[0]):
         xdat += ss[0] + ", "
         for i in range(ny):
            ydat[i] += ss[i+1] + ", "
   xdat  = xdat.rstrip(',') + "]"
   for i in range(ny):
      ydat[i] = ydat[i].rstrip(',') + "]"


   msg4 = "</pre>\n"
   msg4 += '''
