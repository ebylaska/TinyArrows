      os.unlink(freqfilename)
      if (foundir):
         os.unlink(irdosfilename)
         os.unlink(irfreqfilename)
   except:
      print("chemdb_freq: Failed to delete temporary gnuplot files")

   pmsg = msg0 + pmsg1 + msg2
   hmsg = msg0 + hmsg1 + msg2
   return (pmsg,hmsg,jmagelist)

