def esmileswithname_to_esmiles(esmiles0):
   try:
      if esmiles_hasname(esmiles0):
         sss = esmiles0.split()
         ss = []
         bopen = False
         for s in sss:
            if "{" in s:    bopen = True
            if (not bopen): ss.append(s)
            if "}" in s:    bopen = False
            #if "{" not in s:
            #   ss.append(s)
         name = ''
         for ll in range(len(ss)-1):
            name += ss[ll] + "%20"
         name += ss[len(ss)-1]

         esmiles1 = nametosmiles(name)

         if esmiles1=='': esmiles1='C'
         bopen = False
         for s in sss:
            if "{" in s:    bopen = True
            if (bopen): esmiles1 += " " + s
            if "}" in s:    bopen = False
            #if "{" in s:
            #   esmiles1 += " " + s
      else:
         esmiles1 = esmiles0

   except:
      esmiles1 = esmiles0

   return esmiles1


