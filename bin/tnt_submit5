               rxyz.append(y)
               rxyz.append(z)
            bbb = bondings.split()
            nbonds = len(bbb)/3
            sgamma = 0.0
            for b in range(nbonds):
               coef = evalnum(bbb[3*b])
               ii   = evalnum(bbb[3*b+1])-1
               jj   = evalnum(bbb[3*b+2])-1
