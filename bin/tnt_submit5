def eval_dict(dict0,key):
   if key in dict0:
      aak = dict0[key]
   else:
      aak = dict0['.']
   return aak

def evalnum(s):
   try:
      return int(s)
   except ValueError:
      return float(s)

def RepresentsInt(s):
   try: 
      int(s)
      return True
   except ValueError:
      return False




def taskjobtheory(jobtheory,mult):
   if (jobtheory=="ccsd(t)") and (mult==1):
      tt = 'ccsd(t)'
   elif (jobtheory=="ccsd(t)") and (mult>1):
      tt = 'tce'
   elif (jobtheory=="ccsd") and (mult==1):
      tt = 'ccsd'
   elif (jobtheory=="ccsd") and (mult>1):
      tt = 'tce'
   elif (jobtheory=="qsharp_chem"):
      tt = 'tce'
   elif (jobtheory=="mp2") and (mult==1):
      tt = "direct_mp2"
   elif (jobtheory=="hf") or (jobtheory=="uhf"):
      tt = "scf"
   else:
      tt = jobtheory
   return tt


