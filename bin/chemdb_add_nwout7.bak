               eigblob = 'restricted\n'
            if (line.find("Vector") != -1): eigblob += line

            if (pspwset):
               if (len(line.split())>1):
                  eigblob += line
               else:
                  pspwset = False
            elif (line.find("virtual orbital energies:") != -1):
               eigblob += line
               pspwset  = True
            elif (line.find("orbital energies:") != -1):
               eigblob = line
               pspwset = True

         ofile.close()

      except:
         eigblob = ''
   return eigblob


