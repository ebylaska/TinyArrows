def gen_surfaceareas(outfile):
   msurface=[[0.0],[0.0]]
   if (os.path.exists(outfile)):
      count = -1
      xyzdat = []
      ofound = False
      gfound = False
      done = False
      ofile = open(outfile,'r')
      for line in ofile:
        if (not done):
           if (count>0):
              if (len(line)<=5):
                 done = True
              else:
                 xyzdat.append(line)
           if (not done):
              if (count>=0):
                 count += 1
              if (line.find("Optimization converged") != -1):
                 ofound = True
              if (line.find("Failed to converge in maximum number of steps") != -1):
                 ofound = True
              if (ofound and (line.find("No.") != -1)):
                 gfound = True
                 count = 0
      ofile.close()
      n = len(xyzdat)

      if (n>0):
         atoms = []
         for i in range(n):
            split = xyzdat[i].split()
            xyz = []
            xyz.append(split[1])
            xyz.append(evalnum(split[3]))
            xyz.append(evalnum(split[4]))
            xyz.append(evalnum(split[5]))
            atoms.append(xyz)
         n_sphere = 9600
         asas = calculate_asa(atoms, 1.4, n_sphere)
         savs = calculate_sav(atoms, 1.4, n_sphere)
         msurface = [asas,savs]

   return msurface




