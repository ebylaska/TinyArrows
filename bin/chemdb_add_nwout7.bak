      if (issqlite3):
         dbcon = sqlite3.connect(dbfile)
      else:
         host = hup.split()[0]
         user = hup.split()[1]
         pss = hup.split()[2]
         dbcon = MySQLdb.connect(host,user,pss,dbfile)
      with dbcon:
         cur = dbcon.cursor()
         cur.execute(sqlcheck)
         row  = cur.fetchone()
         count = row[0]
      dbcon.close()
      print "MOLECULECOUNT=",count

      with open(countfilename,'w') as ff:
         ff.write("%d" % count)

   except:
      print "update_calculationscount failed"



