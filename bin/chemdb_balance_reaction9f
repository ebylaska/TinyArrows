   for e in redges:
      lstring += shift + "admat1[%s][%s] = 0\n" % (ijk[e[0]],ijk[e[1]])
      lstring += shift + "admat1[%s][%s] = 0\n" % (ijk[e[1]],ijk[e[0]])
