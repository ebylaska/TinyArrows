#            generate_vlist                   #
#                                             #
###############################################
def generate_vlist(n,adjmat,dd):

   vlist = []
   for i in range(n):
      vv = [i]
      for l in range(dd):
         for ii in vv:
            for j in range(n):
               if (adjmat[ii][j]>0): vv.append(j)
      vv = list(set(vv))
      vv.remove(i)
      vlist.append(vv)

   return vlist

