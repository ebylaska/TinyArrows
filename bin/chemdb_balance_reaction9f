#            filter_reaction_string           #
#                                             #
###############################################

def filter_reaction_string(prune,bondstring1):
   ss1 = bondstring1.split(':')
   for i in range(len(prune)):
      if prune[i]==0:
         ss1[i] = ''
   nstring1 = ':'.join(ss1)
   return nstring1

def rotate(l,n):
    return l[n:] + l[:n]






