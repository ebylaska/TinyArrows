   sqlinsert = "insert into " + table + " (name,smiles) values (?,?);"

   insertlist = []
   insertlist.append(name)
   insertlist.append(smiles)

   if (not issqlite3):
      sqlinsert = sqlinsert.replace('?','%s')

   if (issqlite3):
      dbcon = sqlite3.connect(dbfile)
   else:
      host = hup.split()[0]
      user = hup.split()[1]
      pss  = hup.split()[2]
